<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:internal="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="18dp"/>
            <solid android:color="@color/afk"/>
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <corners android:radius="18dp"/>
            <gradient android:startColor="@color/adm" android:endColor="@color/adk"/>
        </shape>
    </item>
</selector>
