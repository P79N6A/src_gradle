<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:internal="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="2dp"/>
            <gradient android:startColor="#f5eee1" android:endColor="#ffe4ad" android:angle="270"/>
        </shape>
    </item>
    <item android:left="1dp" android:top="1dp" android:right="1dp" android:bottom="1dp">
        <shape android:shape="rectangle">
            <corners android:radius="2dp"/>
            <solid android:color="#fe2c55"/>
        </shape>
    </item>
</layer-list>
